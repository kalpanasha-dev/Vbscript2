count=0
i=1
str=inputbox("enter string")
Do
    char=mid(str,i,1)
    msgbox char
    If char="p" Then
        count=count+1
        i=i+1
        msgbox count
        msgbox i
        else
        i=i+1
        msgbox count
        msgbox i
        
    End If
Loop While char<>"t"


