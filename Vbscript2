Set xl=createobject("excel.application")
xl.visible=true
Set wb=xl.Workbooks.open("E:\HP\annai2.xlsx")
Set sh=wb.Worksheets("sheet1")
For i = 2 To 4 Step 1
    For j= 1 To 2 Step 1
        sh.cells(i,j).value=cint(inputbox("enter n1&n2"))
    Next
Next
For k = 2 To 4 
    sh.cells(k,3).value=sh.cells(k,1).value+sh.cells(k,2).value
Next
wb.Save
wb.Close
xl.Quit
 

 
*************************************************************************************
Set xl=createobject("excel.application")
xl.visible=true
Set wb=xl.Workbooks.open("E:\HP\annai2.xlsx")
Set sh=wb.Worksheets("sheet1")
For i = 1 To 1 Step 1
    For j = 1 To 3 Step 1
        sh.cells(i,j).value=inputbox("enter headings")
    Next
Next
For i = 2 To 4 Step 1
    For j= 1 To 2 Step 1
        sh.cells(i,j).value=cint(inputbox("enter n1&n2"))
    Next
Next
For k = 2 To 4 
    sh.cells(k,3).value=sh.cells(k,1).value+sh.cells(k,2).value
Next
wb.Save
wb.Close
xl.Quit
 

 
Set xl=createobject("excel.application")
xl.visible=true
Set wb=xl.Workbooks.open("E:\HP\annai2.xlsx")
Set sh=wb.Worksheets("sheet1")
For i = 1 To 1 Step 1
    For j = 1 To 3 Step 1
        sh.cells(i,j).value=inputbox("enter headings")
        sh.cells(i,j).interior.color=vbyellow
        sh.cells(i,j).font.color=vbgreen
    Next
Next
For i = 2 To 4 Step 1
    For j= 1 To 2 Step 1
        sh.cells(i,j).value=cint(inputbox("enter n1&n2"))
    Next
Next
For k = 2 To 4 
    sh.cells(k,3).value=sh.cells(k,1).value+sh.cells(k,2).value
Next
wb.Save
wb.Close
xl.Quit
 

 
 
 
 
 
Set fso=createobject("scripting.filesystemobject")
For i = 1 To 3 Step 1
    fso.createfolder("E:\test"&i)
Next
 

 
 
 
