Dim Marks
Marks=cint(Inputbox("Enter your Marks"))
If Marks>0 and marks <=49 Then
    msgbox"Fail"
    Else If Marks >=50 and marks<=74 Then
        msgbox"PASS"
        else If Marks>=75 Then
            msgbox "Distinction"
            else
            msgbox "Invalid Input"
        End If
    End If
End If
